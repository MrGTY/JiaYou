<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.zhaojisys.dao.oilrecord.OilrecordMapper">

	<select id="selectAll"  resultType="Oilrecord">
		select * from oilrecord
	</select>
	<select id="getGasstationBy_idAndType" resultType="Gasstation">
		select siteName,detaileAddress from
		gasstation where id=#{id} and gsType=#{gsType}
	</select>
</mapper>